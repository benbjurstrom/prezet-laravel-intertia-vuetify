{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/benbjurstrom/reauthenticate"
      },
      {
        "type": "composer",
        "url": "https://nova.laravel.com"
      }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "benbjurstrom/eloquent-postgres-uuids": "^3.0",
        "darkghosthunter/laraguard": "^1.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "inertiajs/inertia-laravel": "^0.2",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "league/glide": "2.0.x-dev",
        "mpociot/reauthenticate": "dev-master#e54f5bea2050c0fac27a17a6a82077c83048528d",
        "reinink/remember-query-strings": "^0.1.0",
        "tightenco/ziggy": "^0.9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5.5",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "compile": [
            "npm run prod",
            "@php artisan vendor:publish --provider=\"Laravel\\Horizon\\HorizonServiceProvider\"",
            "@php artisan migrate --force"
        ],
        "reseed": [
            "@php artisan migrate:fresh",
            "@php artisan db:seed"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
